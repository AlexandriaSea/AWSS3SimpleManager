<Window x:Class="WenjieZhou_COMP306Lab1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WenjieZhou_COMP306Lab1"
        mc:Ignorable="d"
        Title="COMP 306 Lab 1 by Wenjie Zhou" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Center">
            <Button Content="Bucket Level Operations" Click="BucketLevelOperations_Click" Margin="10"
                    FontSize="16" Background="RoyalBlue" Foreground="Gold" Width="250" Height="50"/>
            <Button Content="Object Level Operations" Click="ObjectLevelOperations_Click" Margin="10"
                    FontSize="16" Background="RoyalBlue" Foreground="Gold" Width="250" Height="50"/>
            <Button Content="Exit" Click="Exit_Click" Margin="10" 
                    FontSize="16" Background="RoyalBlue" Foreground="Gold" Width="150" Height="50"/>
        </StackPanel>
    </Grid>
</Window>